journal = {IEEE Transactions on Vehicular Technology},
keywords = {Building transmission,physical optics (PO),radio propagation,ray tracing,ultrawideband (UWB),uniform theory of diffraction (UTD)},
month = {feb},
number = {2},
pages = {541--552},
title = {{Time-Domain Solution for Transmitted Field Through Low-loss Dielectric Obstacles in a Microcellular and Indoor Scenario for UWB Signals}},
